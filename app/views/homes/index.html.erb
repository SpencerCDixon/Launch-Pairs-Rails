<h1>Launch Pairs</h1>

<table>
  <tr>
    <th>Email</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Status</th>
  </tr>
<% @users.each do |user| %>
  <tr>
    <td> <%= user.email %></td>
    <td> <%= user.first_name %></td>
    <td> <%= user.last_name %></td>
    <td> <%= user.current_status %></td>
  </tr>
<% end -%>
</table>
